{
    "require" : {
        "stylus --version" : true,
        "uglifyjs --version" : true,
        "yuicompressor --version" : true
    },
    "compress" : {
        "css" : "yuicompressor -v -o {dest} {source}",
        "js"  : "uglifyjs -c -o {dest} {source}"
    },
    "process" : {
        "css"  : "cp {source} {dest}",
        "js"   : "cp {source} {dest}",
        "styl" : "stylus {source} -o {dest_dir}"
    },
    "ext" : {
        "styl" : "css"
    },
    "debug" : true
}
