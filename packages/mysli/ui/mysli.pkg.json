        "mysli/output" : ">= 1",
        "mysli/zepto"  : ">= 1",
        "mysli/web"    : ">= 1"
    },

    "factory" : {
        "ui"        : "construct(mysli/tplp, mysli/output, mysli/web)",
        "setup"     : "construct(mysli/web, mysli/event)",
        "script/ui" : "construct(mysli/event)"
    },

    "about" : {
