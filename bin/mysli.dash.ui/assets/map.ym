includes:
    external:
        merge: external.js
        files: "*.ts"
    common/js:
        merge: common.js
        files: "*.ts"
    ui/css:
        merge: ui.css
        files:
            - variables.styl !reload
            - mixins.styl !reload
            - common.styl
            - fonts.css
            - font-awesome.css
            - "*.styl"
            - "*.css"
    ui/js:
        merge: ui.js
        files: [ "*.ts", "*.js" ]
    fonts:
        publish: Yes
        process: No
